{
  "name": "puddle-webapp",
  "version": "1.0.0",
  "description": "Puddle Web App",
  "scripts": {
    "parcel:build": "parcel build src/index.html --public-url / --dist-dir dist",
    "parcel:clean": "rm -rf dist .parcel-cache",
    "parcel:serve": "parcel serve src/index.html --public-url / --dist-dir dist",
    "parcel:watch": "npm run parcel:clean && parcel watch src/index.html --public-url / --dist-dir dist"
  },
  "author": "Pin Tan",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "@types/bootstrap": "^5.2.6",
    "@types/mustache": "^4.2.2",
    "@types/superagent": "^4.1.16",
    "@types/vanillajs-datepicker": "^1.2.2",
    "parcel": "^2.8.2",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^1.7.4",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "chart.js": "^4.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.29.3",
    "mustache": "^4.2.0",
    "superagent": "^8.0.6",
    "vanillajs-datepicker": "^1.3.3"
  }
}
